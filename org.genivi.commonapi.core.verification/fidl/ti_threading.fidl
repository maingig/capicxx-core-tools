package commonapi.threading

interface TestInterface {
    version { major 1 minor 0 }
    
    method testMethod {
        in {
            UInt8 x
        }
        out {
            UInt8 y
        }
    }
    
    broadcast testBroadcast {
        out {
            UInt8 y
        }
    }

    broadcast secondTestBroadcast selective {
        out {
            UInt32 z
        }
    }
}

interface TestInterfaceManager manages TestInterface {
    version { major 1 minor 0 }
}